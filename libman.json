{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/dataTables.bootstrap.min.css",
        "css/jquery.dataTables.min.css",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png",
        "js/dataTables.bootstrap.min.js",
        "js/dataTables.dataTables.js"
      ]
    },
    {
      "library": "boxicons@2.1.4",
      "destination": "wwwroot/lib/boxicons/"
    },
    {
      "library": "sweetalert2@11.10.4",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "sweetalert2.min.js"
      ]
    },
    {
      "library": "Chart.js@4.4.1",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "chart.min.js",
        "helpers.js",
        "helpers.min.js",
        "chart.umd.min.js",
        "chart.umd.js",
        "chart.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "chartjs-chart-matrix@2.0.1",
      "destination": "wwwroot/lib/chartjs-chart-matrix/",
      "files": [
        "dist/chartjs-chart-matrix.min.js"
      ]
    }

  ]
}